{
  "name": "mnaatjes/mvc-skeleton",
  "description": "The skeleton for a new app using the mnaatjes/mvc-framework.",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/mnaatjes/mvc-framework.git"
    }
  ],
  "require": {
    "mnaatjes/mvc-framework": "dev-main"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "prefer-stable": true
}